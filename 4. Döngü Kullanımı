#include <stdio.h>
#include <stdlib.h>

int main() {
    int x;
    printf("50 den kucuk bir pozitif tam sayÄ± giriniz:\n ");
    scanf("%d", &x);

    if(x <= 0 || x > 50) {
        printf("Gecersiz sayi girdisi.\n");
        return 0;
    }

    for(int i = 1; i <= x; i++) {
        if(i % 3 == 0) continue;
        printf("%d ", i);
    }

    return 0;
}
