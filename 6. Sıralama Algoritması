#include <stdio.h>
#include <stdlib.h>

int x,y,z;


    int main()
{

    printf("Karsilastirilacak olan 3 tam sayiyi giriniz:\n");
    scanf("%d %d %d",&x,&y,&z);

    if(z >= y && y >= x){

        printf("%d <= %d <= %d",x,y,z);
    }

    else if(y >= z && z >= x){

        printf("%d <= %d <= %d",x,z,y);
    }

    else if(x >= z && z >= y){

        printf("%d <= %d <= %d",y,z,x);
    }

    else if(z >= x && x >= y){

        printf("%d <= %d <= %d",y,x,z);
    }

    else if(y >= x && x >= z){

        printf("%d <= %d <= %d",z,x,y);
    }

    else if(x >= y && y >= z){

        printf("%d <= %d <= %d",z,y,x);
    }

}
